<UserControl x:Class="inventory.View.Products.ModifyProduct"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
              xmlns:local="clr-namespace:inventory.Controls" 
             mc:Ignorable="d" d:DesignHeight="450" d:DesignWidth="550" Background="Transparent">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>



            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <local:AutoCompleteTextBox Height="23" Width="240" x:Name="textBox1" DelayTime="500" Margin="188,3,122,4" Grid.Row="1" />
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Search Product" VerticalAlignment="Top" Margin="60,10,0,0" Grid.Row="1"/>
    </Grid>

</UserControl>
